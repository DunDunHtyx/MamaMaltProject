page{
  width: 100%;
  height: 100%;
}
/* 提醒 */
#noticeBox{
    border: 1px solid #e2af82b6;
    display: flex;
    width: 95%;
    height: 75rpx;
    margin: 0 auto;
    margin-top: 30rpx;
    border-radius: 10rpx;
    background-color: #fbf2eb;
}
#exclamation{
    width: 45rpx;
    height: 45rpx;
    margin-top: 8rpx;
    margin-left: 10rpx;
}
#noticeText{
  color: #ce781d;
  margin-left: 14rpx;
  margin-top: 12rpx;
  letter-spacing: 6rpx;
  font-size: 28rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

/* 表单 */
#form{
    border: 1px solid #8b888842;
    width: 96%;
    margin: 0 auto;
    margin-top: 40rpx;
    border-radius: 8rpx;
    /* padding: 10rpx; */
  }

  /* 快递公司 */
  #ChoiceCourierCompaniesBox{
        display: flex;
        padding: 30rpx;
        border-bottom: 1px solid #8b888842;
  }
  .CourierCompaniesName{
    font-size: 34rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
  }
  #CourierCompaniesChoose{
      font-size: 34rpx;
      letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
  }
  /* 快递单号 */
  #CourierNumberBox{
        padding: 30rpx 30rpx 30rpx 30rpx;
}
#InputCourierCompaniesNumber{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 32rpx;
    letter-spacing: 4rpx;
    margin-top: 10rpx;
    margin-left: 15rpx;
}

/* 按钮 */
#AddTrackingNumberButton{
    border: 1px solid #e59572;
    width: 60%;
    height: 75rpx;
    line-height: 70rpx;
    border-radius: 10rpx;
    margin: 0 auto;
    margin-top: 100rpx;
    text-align: center;
    background-color: #e59572;
}
#TrackingNumberText{
    font-size: 38rpx;
    letter-spacing: 8rpx;
    color: white;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}