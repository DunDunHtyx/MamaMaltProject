page {
    width: 100%;
    height: 100%;
    background-color: #FFF8F8;
}

#OneTab {
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 34rpx;
    letter-spacing: 6rpx;
    border-bottom: 1px solid rgba(128, 128, 128, 0.226);
}

/* 我买到的 商品内容 */
#MyBuyBox{
    width: 95%;
    margin: 0 auto;
    padding: 20rpx;
    border:1px solid gray;
    margin-top: 40rpx;
    border-radius: 15rpx;
    background-color: white;
}
/* 头部 */
#MybuyTopMoudle{
    display: flex;
    justify-content:space-between;
    padding: 0rpx 10rpx 10rpx 2rpx;
}
#TopMoudle_one{
    display: flex;
    /* border: 1px solid red; */
}
#avaterImage{
        width: 100rpx;
        height: 100rpx;
        border-radius: 60rpx;
}
#nickName{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 30rpx;
    margin-top: 25rpx;
    letter-spacing: 4rpx;
    margin-left: 20rpx;
}
#goodState{
    font-family: AlibabaPuHuiTi-2-45-Light;
    color: #f65706;
    font-size: 33rpx;
    letter-spacing: 4rpx;
    margin-top: 10rpx;
    margin-right: 20rpx;
}
/* 中部 */
#MybuyCentralModule{
        display: flex;
        margin-top: 15rpx;
        padding: 10rpx;
        justify-content: space-between;
}
#GoodImage{
        width: 35%;
        height: 200rpx;
        border-radius: 20rpx;
}
#GoodInclucation{
        width: 60%;
        font-family: AlibabaPuHuiTi-2-45-Light;
        letter-spacing: 6rpx;
        font-size: 30rpx;
        overflow: hidden;
        text-overflow: ellipsis; 
        display: -webkit-box;
        -webkit-line-clamp:5;
        -webkit-box-orient: vertical;
        /* border: 1px solid red; */
}
#AcctulyPay{
        width: 96%;
        /* margin-top: 10rpx; */
        display: flex;
        justify-content: flex-end;
        /* border: 1px solid red; */
}
#AcctullyPayText{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 36rpx;
    letter-spacing: 6rpx;
}
#AcctullyPayNumber{
    font-size: 36rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
/* 底部 */
#MyBuyMiddleBox{
        display: flex;
        justify-content: flex-end;
        margin-top: 20rpx;
}
.MyBuyButton{
        margin-left: 20rpx;
        border-radius: 10rpx;
        width: 190rpx;
        height: 60rpx;
        line-height: 60rpx;
        text-align: center;
        
}
.MyBuy_text{
    font-size: 32rpx;
    letter-spacing: 8rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}



/* 我发布的商品 */
#MyPublishBox{

}
/* 我发布的商品信息模块 */
#MyPublishGoodInfo{
    background-color: white;
    /* border: 1px solid red; */
    border: 1px solid rgba(128, 128, 128, 0.822);
    margin: 0 auto;
    margin-top: 30rpx;
    width: 95%;
    border-radius: 20rpx;
}
#MyPublishGoodInfoItem{
    display: flex;
    justify-content: space-between;
}
#MyPublishGoodImage{
    width: 30%;
    height: 260rpx;
    margin-top: 15rpx;
    margin-left: 20rpx;
    border-radius: 10rpx;
}
#MyPublishGoodRightBox{
        /* border: 1px solid red; */
        width: 65%;
        padding: 10rpx;
}
/* 商品表示 */
#MyPublishGoodNotice{
        margin-left:-20rpx;
        display: flex;
        margin-top: 10rpx;
        font-family: AlibabaPuHuiTi-2-45-Light;
}
.NoticeBox{
        width: 100rpx;
        text-align: center;
        height: 48rpx;
        margin-left: 20rpx;
        /* border: 1rpx solid red; */
        border-radius: 10rpx;
}
.NoticeText{
        font-size: 30rpx;
        letter-spacing: 6rpx;
}
/* 商品描述 */
#MyPublishIncludtion{
    /* padding: 10rpx; */
    margin-top: 10rpx;
    /* border: 1px solid red; */
    width: 98%;
    font-size: 28rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
    overflow: hidden; 
    text-overflow: ellipsis; 
    display: -webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient: vertical;
}
/* 商品价格 */
#MyPublishGoodPrtice{
    font-size: 36rpx;
    /* border: 1px solid red; */
    /* text-align: end; */
    width: 95%;
    margin-top: 10rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
/* 按钮 */
#ButtonModule{
    /* border: 1px solid red; */
    display: flex;
    justify-content:flex-end;
}
.ButtonBox{
     width: 40%;
     height: 55rpx;
     margin-right: 20rpx;
     margin-top: 10rpx;
     line-height: 50rpx;
     text-align: center;
     border-radius: 10rpx;      
}
.ButtonText{
    letter-spacing: 4rpx;
    font-size: 30rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}