page{
  width: 100%;
  height: 100%;
}

#swiperBox{
    position: relative;
    height: 400rpx;
    z-index: -999;
}

.slide-image {
  height: 100%;
  width: 100%;
}
/* 活动信息模块 */
#activityInfor{
    position: relative;
    top: -120rpx;
    border-top: 2px solid #BBBBBB;
    border-radius: 35rpx 35rpx 0 0;
    background-color: white;
}
#InforBOx{
    margin-top: 30rpx;
}
#title{
    font-family: AlibabaPuHuiTi-2-55-Regular;
    font-size: 42rpx;
    letter-spacing: 4rpx;
    font-weight: 600;
    margin-left: 20rpx;
}

#attestationBOx{
  display: flex;
  margin-top: 15rpx;
}
#attestationText{
    font-family: AlibabaPuHuiTi-2-45-Light;
    color: #0F40F5;
    font-weight: 600;
    font-size: 32rpx;
    margin-left: 30rpx;
    letter-spacing: 4rpx;
}
#type{
  margin-left: 30rpx;
    border-radius: 10rpx;
      background-color: #f59c30;
}
#type_text{
    padding: 5rpx;
    padding-left: 15rpx;
    padding-right: 15rpx;
    font-size: 30rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
    color: white;
  }

#LabelBox{
    display: flex;
    width: 96%;
    margin: 0 auto;
    margin-top: 10rpx;
}
.LabelText{
    font-size: 35rpx;
    margin-left: 15rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
    letter-spacing: 4rpx;
}

/* 活动时间 */
#ApplicationBox{
    width: 95%;
    margin: 0 auto;
    margin-top: 10rpx;
}
#TimeBox{
    display: flex;
    margin-left: 10rpx;
}
#TimeBoxText{
  font-size: 33rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}
#Time{
  font-size: 30rpx;
  letter-spacing: 4rpx;
  margin-left: 10rpx;
  margin-top: 5rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}



/* 地址 */
#addressBox{
    display: flex;
    height: 70rpx;
    border-radius: 10rpx;
    width: 94%;
    margin: 0 auto;
    margin-top: 20rpx;
    background-color: #EFEFEF;
}
#addresssImage{
    width: 55rpx;
    height: 55rpx;
    margin-top: 6rpx;
    margin-left: 20rpx;
}
#addressText{
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 32rpx;
  margin-top: 14rpx;
  margin-left: 8rpx;
  letter-spacing: 4rpx;
}

/* 简介 */
#brieflyText{
      width: 40%;
      font-family: AlibabaPuHuiTi-2-55-Regular;
      font-size: 44rpx;
      letter-spacing: 8rpx;
      margin-left: 30rpx;
      margin-top: 30rpx;
    }
#brieflyBox{
      font-family: AlibabaPuHuiTi-2-45-Light;
      width: 90%;
      margin: 0 auto;
      padding: 20rpx;
      font-size: 35rpx;
    }
#brieflyContent{
    letter-spacing: 4rpx;
}  
/* 活动介绍 */
#describesText{
  width: 40%;
  font-family: AlibabaPuHuiTi-2-55-Regular;
  font-size: 44rpx;
  letter-spacing: 8rpx;
  margin-left: 30rpx;
  margin-top: 10rpx;
}
#describesBox{
  font-family: AlibabaPuHuiTi-2-45-Light;
  width: 90%;
  margin: 0 auto;
  padding: 25rpx;
  font-size: 35rpx;
}
#describesContent{
  letter-spacing: 4rpx;
}
/* 预约框 */
#BookBox{
    width: 100%;
    position: fixed;
    top: 91%;
    background-color: white;
    display: flex;
}
#CallBox{
  width: 30%;
  margin-top: 8rpx;
  text-align: center;
}
#CallIcon{
    width:75rpx;
    height:75rpx;
}
#CallText{
  font-size: 33rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}
#book{
      border-radius: 10rpx;
      width: 60%;
      height: 80rpx;
      margin: 0 auto;
      background-color: #E99D42;
      margin-top: 40rpx;
}
#bookText{
    text-align: center;
    font-size: 45rpx;
    line-height: 78rpx;
    color: white;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#dd{
  height: 150rpx;
}