page {
    width: 100%;
    height: 100%;
    background-color: #FFF8F8;
}

/* 第一部分 */
#oneModule {
    width: 95%;
    margin: 0 auto;
    margin-top: 40rpx;
    border: 1px solid gray;
    display: flex;
    height: 360rpx;
    border-radius: 30rpx;
    background-color: white;
}

#goodImage {
    width: 36%;
    height: 300rpx;
    margin-left: 20rpx;
    margin-top: 30rpx;
    border-radius:15rpx;
}

#oneModule_righe {
    width:60%;
    margin-left: 15rpx;
    /* border: 1px solid red; */
}

#goodTitle {
    width: 96%;
    /* border: 1px solid red; */
    letter-spacing: 4rpx;
    margin-top: 30rpx;
    overflow: hidden;
    font-family: AlibabaPuHuiTi-2-55-Regular;
    text-overflow: ellipsis; 
    display: -webkit-box;
    -webkit-line-clamp:3;
    -webkit-box-orient: vertical;
}

#goodStateModule {
    display: flex;
    /* border: 1px solid red; */
    /* justify-content: flex-start; */
}

.orTransportBox {
    width: 130rpx;
    height: 55rpx;
    line-height: 55rpx;
    text-align: center;
    border-radius: 10rpx;
    margin-top: 25rpx;
    /* margin-left: 40rpx; */
    background-color: #55b6f7;
    color: white;
}

.transportText {
    font-size: 30rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}

#qualityJudgment {
    width: 120rpx;
    height: 55rpx;
    line-height: 55rpx;
    text-align: center;
    border-radius: 10rpx;
    margin-top: 25rpx;
    margin-left: 20rpx;
    border: 1px solid red;

}

#qualityJudgmentText {
    color: red;
    letter-spacing: 4rpx;
}
#goodPrtice{
    font-family: AlibabaPuHuiTi-2-65-Medium;
    color: red;
    font-size: 45rpx;
    letter-spacing: 4rpx;
    /* border: 1px solid red; */
    margin-top: 30rpx;
    text-align: end;
    width: 95%;
}

/* 第二部分 */
#twoModule{
    width: 95%;
    margin: 0 auto;
    margin-top: 40rpx;
    border: 1px solid gray;
    border-radius: 30rpx;
    background-color: white;
}
#ReceiptAddressBox{
    padding: 5rpx 10rpx 30rpx 10rpx;
    border-bottom: 1px solid gray;
    display: flex;
    justify-content: space-evenly;
    margin-top: 40rpx;
}
#ReceiptAddressText{
    font-size: 37rpx;
    letter-spacing: 6rpx;
    margin-top: 30rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
#ReceiptAddressInfoBox{
        /* border: 1px solid red; */
        width: 60%;
        margin-left: 20rpx;
}
#ReceiptAddress_Name{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 33rpx;
}
#ReceiptAddress_Address{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 30rpx;
}
#iconImage{
    width: 50rpx;
    height:50rpx;
    margin-top: 30rpx;
    margin-left: 10rpx;
}
/* 运费 */
#freightBox{
    display: flex;
    margin-top: 40rpx;
}
#freightText{
    font-size: 40rpx;
    letter-spacing: 6rpx;
    margin-top: 5rpx;
    margin-left: 30rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
#freightPrtice{
    font-family: AlibabaPuHuiTi-2-65-Medium;
    color: red;
    font-size: 45rpx;
    letter-spacing: 4rpx;
    /* border: 1px solid red; */
    margin-top: 5rpx;
    margin-left: 80rpx;
}

/* 底部栏 */
#buttonBox{
    border-top: 1px solid gray;
    background-color: white;
    position: fixed;
    width: 100%;
    bottom: 0;
    height: 140rpx;
}
#totalBox{
    display: flex;
    justify-content: space-around;
    /* justify-content: flex-end; */
}
#totalPrticeBox{
    display: flex;
    margin-right: 130rpx;
    /* border: 1px solid red; */
}
#totalText{
    font-size: 40rpx;
    letter-spacing: 4rpx;
    margin-top: 28rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
#totalPrtice{
    font-size: 55rpx;
    margin-top: 6rpx;
    font-family: AlibabaPuHuiTi-2-65-Medium;
    color: red;
}
#yesBuy{
    width: 180rpx;
    height: 80rpx;
    line-height: 75rpx;
    border-radius: 10rpx;
    text-align: center;
    margin-top: 22rpx;
    margin-left:35rpx;
    background-color: #f5981f;
    /* border: 1px solid red; */
}
#yesBuyText{
    color: white;
    font-size: 33rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}