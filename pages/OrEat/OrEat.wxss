/* 页面背景颜色为 #f2f2f2 */
page {
  background: #fff8f870;
}

/* 设置导航外层样式，绝对定位设置为fixed。z-index: 999999; 能够将 nav 布局层级结果放在最上层，避免被下层view遮挡*/
.nav {
  position: relative;
  position: fixed;
  background: white;
  font-size: 35rpx;
  color: #353535;
  z-index: 999999;
  max-height: 94rpx;
  font-style: AlibabaPuHuiTi-2-45-Light;
  width: 100%;
  height: 100rpx;
  border-bottom-left-radius: 40rpx;
  border-bottom-right-radius: 40rpx;
  border-bottom: 2px solid rgba(187, 187, 187, 0.5);
  box-shadow: 0px 0px 0px 0 rgba(0, 0, 0, 0);
}

/* 设置为水平布局，文字居中显示 */
.flex {
  display: flex;
  text-align: center;
}

/* 设置选中项的底部边框 */
.nav .nav-item.current {
  border-bottom: 4rpx solid;
}

/* 设置选中项的文字颜色和大小 */
.text-selected {
  color: #F7A513;
  font-size: 35rpx;
}

.Label.selected {
  background-color: #F7A513;
  color: white;
}

/* 设置各项的高度、边距，同时设置flex=1，表示各项平分 */
.nav .nav-item {
  height: 75rpx;
  width: 14%;
  display: inline-block;
  line-height: 90rpx;
  margin: 0 38rpx;
  padding: 0 5rpx;
  margin-top: -10rpx;
}

.tap {
  width: 92%;
  height: 95rpx;
  border-bottom: 1px solid #BBBBBB;
  position: absolute;
  margin-top: 100rpx;
  margin-left: 4%;
}

.Label {
  text-align: center;
  font-size: 33rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-55-Regular;
  line-height: 50rpx;
  margin-right: 20rpx;
  margin-top: 15rpx;
  width: 150rpx;
  height: 50rpx;
  box-shadow: 2px 2px 2px #f7a6c477;
  border-radius: 100rpx;
  background-color: white;
  border: 1px solid #feebf0;
}

.shopping {
  height: 100%;
  width: 92%;
  margin-top: 230rpx;
  margin-left: 4%;
  position: absolute;
}

.shopping-item {
  margin-bottom: 25rpx;
  display: flex;
  width: 100%;
  height: 235rpx;
  border-bottom: 2rpx solid #c9c9c9;
}

.shopping-lift {
  width: 66%;
  height: 200rpx;
}

.shopping-tittle {
  width: 100%;
  font-family: AlibabaPuHuiTi-2-55-Regular;
  font-size: 38rpx;
  letter-spacing: 8rpx;
  height: 60rpx;
}

.shopping-Features {
  width: 100%;
  margin-top: 8rpx;
  height: 140rpx;
}

.Features-item {
  margin-bottom: 10rpx;
  width: 100%;
  height: 60rpx;
  display: flex;
}

.item-item {
  display: flex;
  height: 60rpx;
}
.item-items{
  display: flex;
  margin-left:55rpx;
  height: 60rpx;
}

.text {
  line-height: 65rpx;
  margin-left: 15rpx;
  font-size: 33rpx;
  font-family:AlibabaPuHuiTi-2-45-Light ;
}

.shopping-img {
  width: 34%;
  margin-right: 15rpx;
  margin-top: 20rpx;
  height: 200rpx;
  /* border: 1px solid red; */
}