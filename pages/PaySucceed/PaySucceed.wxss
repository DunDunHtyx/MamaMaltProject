@font-face {
  font-family: AlibabaPuHuiTi-2-45-Light;
  src: url(https://puhuiti.oss-cn-hangzhou.aliyuncs.com/AlibabaPuHuiTi-2/AlibabaPuHuiTi-2-45-Light/AlibabaPuHuiTi-2-45-Light.eot) format('embedded-opentype'),
    url(https://puhuiti.oss-cn-hangzhou.aliyuncs.com/AlibabaPuHuiTi-2/AlibabaPuHuiTi-2-45-Light/AlibabaPuHuiTi-2-45-Light.otf) format('opentype'),
    url(https://puhuiti.oss-cn-hangzhou.aliyuncs.com/AlibabaPuHuiTi-2/AlibabaPuHuiTi-2-45-Light/AlibabaPuHuiTi-2-45-Light.ttf) format('TrueType'),
    url(https://puhuiti.oss-cn-hangzhou.aliyuncs.com/AlibabaPuHuiTi-2/AlibabaPuHuiTi-2-45-Light/AlibabaPuHuiTi-2-45-Light.woff) format('woff'),
    url(https://puhuiti.oss-cn-hangzhou.aliyuncs.com/AlibabaPuHuiTi-2/AlibabaPuHuiTi-2-45-Light/AlibabaPuHuiTi-2-45-Light.woff2) format('woff2');
}

page {
  width: 100%;
  height: 100%;
}

/* 活动信息模块 */
#activityInfoBox {
  background-color: #f0416a;
  height: 350rpx;
  width: 100%;
  border: 1px solid #f0416a;
}

/* 活动信息 */
#infoBox_one {
  width: 96%;
  margin-left: 10rpx;
  border-bottom: 1px solid white;
  padding-left: 10rpx;
  padding-right: 20rpx;
  margin-top: 30rpx;
}

#info_title {
  font-size: 33rpx;
  letter-spacing: 3rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  color: white;
}

#info_address {
  margin-top: 10rpx;
  color: white;
  padding-bottom: 20rpx;
  font-size: 28rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

/* 订单信息 */
#orderBox {
  width: 98%;
  margin: 0 auto;
  margin-top: 20rpx;
  padding-left: 15rpx;
}

#order_matches {
  font-size: 33rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  color: white;
}

#order_ticket {
  font-size: 30rpx;
  letter-spacing: 5rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  color: white;
  margin-top: 4rpx;
}

/*订单信息模块 */
#orderIngoBox {
  border: 1px solid white;
  margin-top: -40rpx;
  border-radius: 25rpx 25rpx 0 0;
  background-color: white;
}

/* 服务 */
#service {
  display: flex;
  width: 96%;
  margin: 0 auto;
  margin-top: 30rpx;
  padding-bottom: 10rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.336);
}

#serviceText {
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 28rpx;
  font-weight: 600;
  letter-spacing: 8rpx;
  margin-left: 20rpx;
}

#serviceBox_one {
  display: flex;
}

#serviceIcon {
  width: 45rpx;
  height: 45rpx;
  margin-top: -2rpx;
  margin-left: 4rpx;
}

#serviceBoxone_text {
  font-size: 26rpx;
  letter-spacing: 5rpx;
  margin-top: 2rpx;
  margin-left: 8rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}


/* 配送方式 */
#distribution {
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.342);
}

#ddd {
  height: 20rpx;
}

#DistributionText {
  font-size: 30rpx;
  letter-spacing: 4rpx;
  font-weight: 600;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#DistributionTicket {
  display: flex;
  margin-top: 10rpx;
}

#DistributionTicketText {
  font-size: 33rpx;
  font-weight: 600;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#DistributionTicket_one {
  margin-left: 30rpx;
  padding-right: 15rpx;
  padding-left: 15rpx;
  line-height: 45rpx;
  border: 1px solid #db9718;
  border-radius: 20rpx;
}

#DistributionTicket_oneText {
  color: #db9718;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 25rpx;
}

#distributionNotice {
  margin-top: 10rpx;
  letter-spacing: 4rpx;
  color: gray;
  font-size: 30rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

/* 联系人 */
#callBox {
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.336);
}

#dddd {
  height: 20rpx;
}

#CallName {
  font-size: 32rpx;
  font-weight: 600;
  letter-spacing: 6rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#CallNameInput {
  margin-top: 10rpx;
  font-size: 30rpx;
  letter-spacing: 4rpx;
  color: gray;
  padding-left: 10rpx;
}

#CallPhoneBox {
  margin-top: 20rpx;
}

#CallPhone {
  font-size: 32rpx;
  letter-spacing: 6rpx;
  font-weight: 600;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#phoneNumber {
  margin-top: 10rpx;
  font-size: 30rpx;
  letter-spacing: 6rpx;
  color: gray;
  padding-left: 10rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

/* 提醒 */
#warmNotice {
  margin-top: 30rpx;
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#warmNoticetext {
  font-size: 25rpx;
  color: gray;
  letter-spacing: 4rpx;
}

/* 提交订单 */
#payBox {
  display: flex;
  border-top: 1px solid rgba(128, 128, 128, 0.377);
  position: fixed;
  width: 100%;
  top: 92%;
  height: 110rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}

#prticeBox {
  margin-top: 30rpx;
  margin-left: 40rpx;
}

#prticeNumber {
  color: #f0416a;
  font-weight: 600;
  font-size: 42rpx;
  letter-spacing: 4rpx;
}

#payButtone {
  width: 25%;
  text-align: center;
  height: 60rpx;
  border-radius: 15rpx;
  margin-left: 320rpx;
  margin-top: 20rpx;
  background-color: #fd2975;
  border: 1px solid red;
}

#payButtonText {
  color: white;
  font-size: 31rpx;
  letter-spacing: 5rpx;
  padding-left: 10rpx;
  line-height: 56rpx;
}