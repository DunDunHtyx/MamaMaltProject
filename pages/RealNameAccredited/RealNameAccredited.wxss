page{
  width: 100%;
  height: 100%;
}
/* 提醒 */
#noticeBox{
    border: 1px solid #e2af82b6;
    display: flex;
    width: 95%;
    height: 55rpx;
    margin: 0 auto;
    margin-top: 30rpx;
    border-radius: 10rpx;
    background-color: #fbf2eb;
}
#exclamation{
    width: 35rpx;
    height: 35rpx;
    margin-top: 8rpx;
    margin-left: 10rpx;
}
#noticeText{
  color: #ce781d;
  margin-left: 12rpx;
  margin-top: 7rpx;
  letter-spacing: 4rpx;
  font-size: 27rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
}
/* 表单 */
#form{
  border: 1px solid #8b888842;
  width: 96%;
  margin: 0 auto;
  margin-top: 40rpx;
  border-radius: 8rpx;
}
/* 填写项 */
.input_item_box{
  display: flex;
  padding: 30rpx;
  border-bottom: 1px solid #cec7c75b;
}
.input_item_title{
  font-size: 33rpx;
  letter-spacing: 4rpx;
	font-family: AlibabaPuHuiTi-2-55-Regular;
}
/* 详细地址 */
#DetailAddress_box{
  padding: 30rpx;
  border-bottom: 1px solid #cec7c75b;
}
#DetailAddress_title{
  font-size: 33rpx;
  letter-spacing: 4rpx;
	font-family: AlibabaPuHuiTi-2-55-Regular;
}
#DetailAddress_input{
  height: 150rpx;
  width: 95%;
  margin: 0 auto;
  margin-top: 10rpx;
  font-size: 33rpx;
  letter-spacing: 4rpx;
  font-family: AlibabaPuHuiTi-2-55-Regular;
}
.input_item_input{
  font-size: 29rpx;
  width: 70%;
  color: grey;
  height: 50rpx;
  margin-left: 30rpx;
  letter-spacing: 4rpx;
  padding-left: 15rpx;
}
/* 同意 */
#agreeBox{
  display: flex;
}
#agreeCheck{
  margin-top: 32rpx;
  margin-left: 26rpx;
	font-family: AlibabaPuHuiTi-2-55-Regular;
}
#agreeText{
  font-size: 26rpx;
  letter-spacing: 4rpx;
  color: grey;
  margin-top: 26rpx;
	font-family: AlibabaPuHuiTi-2-55-Regular;
}
#agrement{
  color: #f10668;
}
#dd{
  height: 30rpx;
}
/* 提交按钮 */
#Yes{
  width: 55%;
  height: 78rpx;
  background-color: #f79d0d;
  margin-top: 150rpx;
}
#yesText{
  font-size: 40rpx;
  line-height: 75rpx;
  text-align: center;
  letter-spacing: 8rpx;
  color: white;
  font-family: AlibabaPuHuiTi-2-55-Regular;
}