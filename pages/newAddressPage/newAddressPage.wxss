page{
    width: 100%;
    height: 100%;
}
#fromBox{
    width: 95%;
    margin: 0 auto;
    margin-top: 60rpx;
    /* border: 1px solid red; */
    /* padding: 30rpx; */
}
/* 收货人 手机号 */
.oneItemBox{
    padding: 20rpx;
    border-bottom: 1px solid gray;
    display: flex;
}
.oneItem_name{
    font-size: 38rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
.oneItem_input{
    font-size: 33rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
    margin-left: 30rpx;
    margin-top: 5rpx;
}

/* 地区 */
#pickerName{
    /* border: 1px solid red; */
    margin-left: 40rpx;
    font-size: 33rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
    margin-top: 5rpx;
}
/* 详细地址 */
#detailBOx{
    /* border: 1px solid red; */
    padding: 20rpx;
    border-bottom: 1px solid gray;
}
#detailAddressInput{
    /* border: 1px solid red; */
    height: 130rpx;
    font-size: 33rpx;
    letter-spacing: 4rpx;
    padding: 20rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
/* 新增地址 */
#newAddAddressButton{
    text-align: center;
    /* border: 1px solid red; */
    width: 75%;
    height: 85rpx;
    line-height: 80rpx;
    margin: 0 auto;
    margin-top: 200rpx;
    border-radius: 10rpx;
    background-color: #e59572;
}
#newAddAddText{
    color: white;
    font-family: AlibabaPuHuiTi-2-55-Regular;
    font-size: 45rpx;
    letter-spacing: 4rpx;
}