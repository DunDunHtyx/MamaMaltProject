page{
    width: 100%;
    height: 100%;
    background-color: #FFF8F8;
}
/* 商品信息 */
#OldGoodInfoModule{
    
}
#oldGoodInfoBox{
    border-bottom: 1px solid rgba(128, 128, 128, 0.486);
    margin-top: 20rpx;
    padding: 30rpx 30rpx 30rpx 15rpx;
    display: flex;
    justify-content: space-between;
    background-color: white;
}
#oldGoodImage{
    width: 28%;
    height: 180rpx;
    /* border: 1px solid red; */
    border-radius: 10rpx;
}
#oldGoodBasicBox{
    width: 50%;
    margin-left:-20rpx;
    /* border: 1px solid red; */
}
#oldGoodTitle{
    overflow: hidden; 
    text-overflow: ellipsis; 
    display: -webkit-box;
    -webkit-line-clamp:3;
    -webkit-box-orient: vertical;
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 28rpx;
    /* font-weight: 600; */
    letter-spacing: 4rpx;

}
#oldGoodPrtice{
    font-size: 35rpx;
    letter-spacing: 4rpx;
    margin-top: 10rpx;
    /* text-align: end; */
	font-family: AlibabaPuHuiTi-2-55-Regular;
}
#oldGoodBuyState{
    width: 16%;
    /* border: 1px solid red; */
	font-family: AlibabaPuHuiTi-2-55-Regular;
    font-size: 33rpx;
    margin-top: 10rpx;
    letter-spacing: 4rpx;
    color: #D25565;
}
#oldGoodBasicPrtice{
    border-bottom: 1px solid rgba(128, 128, 128, 0.486);
    background-color: white;
    /* margin-top: 30rpx; */
}
.oldGoodPrticeItem{
    width: 90%;
    margin: 0 auto;
    /* padding: 10rpx; */
    display: flex;
    margin-top: 10rpx;
    justify-content: space-between;
}
#oldGoodTitlePrticeText{
    font-size: 34rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#oldGoodTitlePrtice{
    font-size: 34rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}

#oldGoodFreightText{
    font-size: 34rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#oldGoodFright{
    font-size: 34rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#oldGoodActuallyText{
    font-size: 36rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
#oldGoodActuallt{
    font-size: 36rpx;
    letter-spacing: 4rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}
/* 收货地址 */
#shipmentsBox{
    background-color: white;
    margin-top: 40rpx;
    border-bottom: 1px solid rgba(128, 128, 128, 0.486);
    border-top: 1px solid rgba(128, 128, 128, 0.486);
    padding: 30rpx;
}
#shipmentTitle{
    font-family: AlibabaPuHuiTi-2-55-Regular;
    font-size: 36rpx;
    letter-spacing: 6rpx;
}
#shipmentInfo{
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#shipment_one{
    font-size: 30rpx;
    letter-spacing: 4rpx;
    margin-top: 6rpx;
}
#shipment_two{
    font-size: 30rpx;
    letter-spacing: 4rpx;
    /* border: 1px solid red; */
    width: 95%;
    margin-top: 6rpx;
}
/* 邮递单号信息 */
#TrackingInfoBox{
    padding: 5rpx 10rpx 10rpx 10rpx;
}
.TrackingItem_one{
    display: flex;
}
.TrackingItem_two{
    margin-top: 5rpx;
}
.TrackingItem_Textone{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 33rpx;
    letter-spacing: 8rpx;
}
.TrackingItem_Texttwo{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 34rpx;
    letter-spacing: 4rpx;
}





/* 订单信息 */
#orderBasicInfo{
    background-color: white;
    margin-top: 40rpx;
    border-bottom: 1px solid rgba(128, 128, 128, 0.486);
    border-top: 1px solid rgba(128, 128, 128, 0.486);
    padding: 30rpx;
}
#orderTopBox{
    display: flex;
    justify-content: space-between;
}
#orderTop_title{
    font-family: AlibabaPuHuiTi-2-55-Regular;
    font-size: 36rpx;
    letter-spacing: 6rpx;
}
#orderTop_call{
    border: 1px solid #ca3e47;
    width: 200rpx;
    height: 55rpx;
    line-height: 55rpx;
    border-radius: 20rpx;
    text-align: center;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
#orderTop_callText{
    font-size: 32rpx;
    letter-spacing: 4rpx;
    color: #ca3e47;
}
#orderButtomBox{
    margin-top: 30rpx;
    /* border: 1px solid red; */
    width: 95%;
}
.orderButtomItem{
    display: flex;
    margin-top: 8rpx;
    justify-content: space-between;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
.orderButtom_one{
    font-size: 32rpx;
    letter-spacing: 6rpx;
}
.orderButtom_two{
    font-size: 28rpx;
    letter-spacing: 2rpx;
}

/* 选项按钮 */
#shocieButtonBox{
    display: flex;
    justify-content: space-around;
    margin-top: 80rpx;
}
.ButtonItem{
    width: 45%;
    text-align: center;
    border-radius: 10rpx;
    height: 65rpx;
    line-height: 60rpx;
    /* border: 1px solid red; */
}
.ButtonText{   
    font-size: 36rpx;
    letter-spacing: 6rpx;
    font-family: AlibabaPuHuiTi-2-55-Regular;
}