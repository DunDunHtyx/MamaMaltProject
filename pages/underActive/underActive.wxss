page {
  width: 100%;
  height: 100%;
  background-color: #FFF8F8;
}

/* 第一个Tab栏 */
#OneTab {
  margin-top: -20rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 34rpx;
  letter-spacing: 6rpx;
  height: 110rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.548);
}

/* 第二个Tab栏 */
#TwoTab {
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 34rpx;
  letter-spacing: 6rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.226);
}

/* 我组织的内容页 */
.organContentModule {
  /* border: 1px solid red; */
  margin-top: 20rpx;
}

.organContentBox {
  border: 1px solid #BBBBBB;
  width: 95%;
  margin: 0 auto;
  margin-top: 40rpx;
  border-radius: 25rpx;
  background-color: white;
}

/* 头部 */
.organTopBox {
  /* border: 1px solid red; */
  justify-content: space-between;
  display: flex;
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 34rpx;
  letter-spacing: 4rpx;
  margin-top: 28rpx;
}

.organTopBox_one {
  display: flex;
  /* border: 1px solid red; */
  margin-left: 20rpx;
}

.organTopTitle {
  margin-left: 15rpx;
}

.organType {
  /* border: 1px solid red; */
  padding: 5rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  background-color: #f7581ec4;
  color: white;
  margin-left: -40rpx;
  border-radius: 20rpx;
  font-size: 26rpx;
  letter-spacing: 6rpx;
}

.UpdateAct{
  /* border: 1px solid red; */
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}

/* 中部 */
.organMiddle {
  width: 92%;
  margin: 0 auto;
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 34rpx;
  letter-spacing: 4rpx;
  margin-top: 25rpx;
  /* border: 1px solid red; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* 底部 */
.oragnBottomBox {
  /* border: 1px solid red; */
  margin-top: 30rpx;
  display: flex;
}

.bottomRight_box {
  width: 65%;
  height: 160rpx;
  border-radius: 20rpx;
  border: 1px solid #BBBBBB;
  display: flex;
  font-family: AlibabaPuHuiTi-2-45-Light;
  margin-left: 30rpx;
}

.bottomRight_image {
  width: 160rpx;
  height: 130rpx;
  border-radius: 20rpx;
  margin-top: 14rpx;
  margin-left: 15rpx;
}

.bottomRight_left {
  width: 56%;
  height: 140rpx;
  font-family: AlibabaPuHuiTi-2-45-Light;
  /* border: 1px solid red; */
  margin-left: 20rpx;
  margin-top: 10rpx;
  letter-spacing: 4rpx;
}

.bottomRight_time {
  font-size: 29rpx;
  margin-top: 15rpx;
}

.bottomRight_address {
  margin-top: 20rpx;
  font-size: 29rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.bottomLeft_box {
  /* border: 1px solid red; */
  border-radius: 10rpx;
  padding: 5rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  height: 60rpx;
  color: white;
  font-size: 30rpx;
  letter-spacing: 4rpx;
  text-align: center;
  font-family: AlibabaPuHuiTi-2-45-Light;
  background-color: #f51f0cee;
  margin-top: 45rpx;
  margin-left: 30rpx;
}

.bottomLeft_text {
  margin-top: 5rpx;
}

/* 参加的活动内容信息 */
.TakPartActModule{
    /* border: 1px solid red; */
}
.TakPartActBox{
  border: 1px solid #BBBBBB;
  width: 95%;
  margin: 0 auto;
  margin-top: 40rpx;
  border-radius: 25rpx;
  background-color: white;
}
/* 头部 */
.takePartTop{
    border-bottom: 1px solid gray;
    display: flex;
    padding: 30rpx;
    justify-content: space-between;
  }
.JoinActTitle{
  font-family: AlibabaPuHuiTi-2-55-Regular;
  overflow: hidden; 
  text-overflow: ellipsis; 
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  /* border: 1px solid red; */
  font-size: 33rpx;
  letter-spacing: 4rpx;
  width: 75%;
}
.JoinOrderState{
  font-family: AlibabaPuHuiTi-2-45-Light;
  font-size: 35rpx;
  letter-spacing: 4rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
}
/* 中部 */
.takPartMiddle{
  display: flex;
  justify-content:end;
  padding:20rpx 30rpx 30rpx 30rpx;
}
.JoinActImage{
    width: 170rpx;
    height: 210rpx;
    border-radius: 10rpx;
}
.JoinOrderDetailBOx{
    width:55%;
    margin-left: 40rpx;
    /* border: 1px solid red; */
  }
  .orderItem{
    font-family: AlibabaPuHuiTi-2-45-Light;
    font-size: 30rpx;
    letter-spacing: 6rpx;
    overflow: hidden;
    text-overflow: ellipsis; 
    display: -webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient: vertical;
}
/* 底部 */
.takPartBottom{
    width: 96%;
    display: flex;
    justify-content:flex-end;
    font-family: AlibabaPuHuiTi-2-45-Light;
}
.ButtonItem{
  padding: 10rpx;
  /* border: 1px solid #727070; */
  border-radius: 20rpx;
  margin-left: 30rpx;
}
.ButtonItem_text{
  font-size: 30rpx;
  letter-spacing: 2rpx;
}


